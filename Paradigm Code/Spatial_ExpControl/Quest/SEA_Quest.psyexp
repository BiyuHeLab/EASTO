        <Param name="Begin JS Routine" updates="constant" val="/* Syntax Error: Fix Python code */" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="# Randomize Order of FAOH &amp;#10;#Initialize cat components&amp;#10;win.callOnFlip(tk.sendMessage, 'qCatOnset')&amp;#10;if probe_loc == 'L':&amp;#10;    probe_cue_img_2 = '../cues/cue_left.png'&amp;#10;    &amp;#10;elif probe_loc == 'R':&amp;#10;    probe_cue_img_2 = '../cues/cue_right.png'&amp;#10;&amp;#10;cats = ['Face', 'Animal', 'Object', 'House'] &amp;#10;shuffle(cats)&amp;#10;cat_question = (&amp;#10;                u'Image Category:\n\n\n\n\n\n\n\n'&amp;#10;                u'{cats1:\u00a0^9} {cats2:\u00a0^9} {cats3:\u00a0^9} {cats4:\u00a0^9}\n\n\n'&amp;#10;                u'{b1:\u00a0^9} {b2:\u00a0^9} {b3:\u00a0^9} {b4:\u00a0^9}'.format(cats1 = cats[0].encode().decode('utf-8'), &amp;#10;                                                           cats2 = cats[1].encode().decode('utf-8'), &amp;#10;                                                           cats3 = cats[2].encode().decode('utf-8'), &amp;#10;                                                           cats4 = cats[3].encode().decode('utf-8'),&amp;#10;                                                           b1 = u'1',b2 = u'2',b3 = u'3',b4 = u'4'&amp;#10;                                                            ))" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="buttons = [&quot;6&quot;, &quot;7&quot;, &quot;8&quot;, &quot;9&quot;];&amp;#10;categories = function () {&amp;#10;    var _pj_a = [], _pj_b = cats;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var c = _pj_b[_pj_c];&amp;#10;        _pj_a.push(c[0]);&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;button_map = dict(zip(categories, buttons));&amp;#10;cat_map = dict(zip(buttons, categories));&amp;#10;if ((probe_loc === &quot;L&quot;)) {&amp;#10;    corAns = button_map[target_left[10]];&amp;#10;} else {&amp;#10;    if ((probe_loc === &quot;R&quot;)) {&amp;#10;        corAns = button_map[target_right[10]];&amp;#10;    }&amp;#10;}&amp;#10;correct_category = cat_map[corAns];&amp;#10;thisExp.addData(&quot;objective_category&quot;, correct_category);&amp;#10;if (cat_resp.keys) {&amp;#10;    category_response = cat_map[cat_resp.keys.slice((- 1))[0]];&amp;#10;    thisExp.addData(&quot;category_response&quot;, category_response);&amp;#10;} else {&amp;#10;    category_response = null;&amp;#10;    thisExp.addData(&quot;category_response&quot;, category_response);&amp;#10;}&amp;#10;if ((correct_category === category_response)) {&amp;#10;    thisExp.addData(&quot;category_correct&quot;, 1);&amp;#10;    category_correct_count += 1;&amp;#10;} else {&amp;#10;    thisExp.addData(&quot;category_correct&quot;, 0);&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# Map buttons to shuffled cats, assumed read L-R&amp;#10;&amp;#10;buttons = ['6','7','8','9']&amp;#10;categories = [c[0] for c in cats]&amp;#10;&amp;#10;button_map = dict(zip(categories,buttons))&amp;#10;cat_map  = dict(zip(buttons,categories))&amp;#10;#&amp;#10;if probe_loc == 'L':&amp;#10;    corAns = button_map[target_left[10]] # Index first letter after &quot; images/&quot; in target&amp;#10;    &amp;#10;elif probe_loc == 'R':&amp;#10;    corAns = button_map[target_right[10]] # Index first letter after &quot; images/&quot; in target&amp;#10;&amp;#10;correct_category = cat_map[corAns]&amp;#10;thisExp.addData('objective_category', correct_category)&amp;#10;&amp;#10;if cat_resp.keys:&amp;#10;    category_response = cat_map[cat_resp.keys[-1]]&amp;#10;    thisExp.addData('category_response', category_response)&amp;#10;&amp;#10;else: #If no response&amp;#10;    category_response = None&amp;#10;    thisExp.addData('category_response', category_response)&amp;#10;&amp;#10;&amp;#10;#Calculate categorization accuracy&amp;#10;if correct_category == category_response:&amp;#10;    thisExp.addData('category_correct', 1)&amp;#10;    category_correct_count += 1 &amp;#10;else:&amp;#10;    thisExp.addData('category_correct', 0)&amp;#10;" valType="extendedCode"/>
