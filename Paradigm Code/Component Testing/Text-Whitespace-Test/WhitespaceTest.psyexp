        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="/* Syntax Error: Fix Python code */" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="#Determine probe cue &amp;#10;&amp;#10;cat_trunc = ['F','A', 'O', 'H']&amp;#10;cat_full = ['Face', 'Animal', 'Object', 'House'] &amp;#10;&amp;#10;block_catmap = dict(zip(cat_trunc,cat_full))&amp;#10;&amp;#10;probe_loc = 'c1'&amp;#10;if probe_loc == 'c1':&amp;#10;    probe_cue_txt = cat_comb[0]&amp;#10;    &amp;#10;elif probe_loc == 'c2':&amp;#10;    probe_cue_txt = cat_comb[1]&amp;#10;&amp;#10;# Randomize Order of subcategory possible responses&amp;#10;#Initialize cat components&amp;#10;&amp;#10;if probe_cue_txt == 'F':&amp;#10;    cats = ['Male', 'Female', 'X'] &amp;#10;&amp;#10;elif probe_cue_txt == 'H':&amp;#10;    cats= ['House', 'Building', 'X'] &amp;#10;&amp;#10;elif probe_cue_txt =='A':&amp;#10;    cats = ['Mammal', 'Non-Mammal', 'X'] &amp;#10;&amp;#10;&amp;#10;elif probe_cue_txt == 'O':&amp;#10;    cats = ['Handheld', 'Non-Handheld', 'X'] &amp;#10;&amp;#10;shuffle(cats)&amp;#10;cat_question = (&amp;#10;                u'Image Category:\n\n\n\n\n'&amp;#10;                u'{cats1:\u00a0^15} {cats2:\u00a0^15} {cats3:\u00a0^15}\n\n\n'&amp;#10;                u'{b1:\u00a0^15} {b2:\u00a0^15} {b3:\u00a0^15}'.format(cats1 = cats[0].encode().decode('utf-8'), &amp;#10;                                                           cats2 = cats[1].encode().decode('utf-8'), &amp;#10;                                                           cats3 = cats[2].encode().decode('utf-8'), b1 = u'1',b2 = u'2',b3 = u'3', &amp;#10;                                                            ))&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="/* Syntax Error: Fix Python code */" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="## Map buttons to shuffled cats, assumed read L-R&amp;#10;#&amp;#10;buttons = ['6','7','8']&amp;#10;categories = [c[0] for c in cats]&amp;#10;&amp;#10;button_map = dict(zip(categories,buttons))&amp;#10;cat_map  = dict(zip(buttons,categories))&amp;#10;#&amp;#10;## Set correct answer dependent on if target is congruent with probe&amp;#10;#if probe_loc == 'c1':&amp;#10;#    corAns = button_map[probe_cue_txt] # Index 2nd letter after &quot; images/&quot; in target&amp;#10;#    &amp;#10;#elif probe_loc != 'c1':&amp;#10;#    corAns = button_map['X'] # Index first letter after &quot; images/&quot; in target&amp;#10;#&amp;#10;#trials.addData('cat_correct', cat_map[corAns])&amp;#10;#&amp;#10;#if cat_resp.keys:&amp;#10;#    trials.addData('cat_select', cat_map[cat_resp.keys[-1]])&amp;#10;#&amp;#10;#&amp;#10;#    &amp;#10;#" valType="extendedCode"/>
